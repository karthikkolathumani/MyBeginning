#include<stdio.h>

int main() {
    int d,k=0;
  scanf("%d",&d);
 
  if(d>2){
     
  for (int i=2;i<d;i++){
      if(d%i==0){
          k=0;
      }
      else
      {
          k=1;
      }
      
  }
  }
  else if(d==2){
  k=0;
  }
 else
 printf("Enter a Prime number");
 return 0;
  
  if(k==1){
      printf("Not Prime number");
  }
  else
  printf("prime");
}

